augroup localfile
	autocmd!
    autocmd FileType *.md setfiletype markdown
 	autocmd FileType *.tex setfiletype tex
	autocmd FileType *.c setfiletype c
	autocmd FileType *.scala setfiletype scala
	autocmd FileType *.v setfiletype coq
	autocmd FileType *.sml setfiletype sml
    autocmd FileType *.snip setfiletype neosnippet
    autocmd FileType *.bib setfiletype bibtex
    autocmd FileType *.vim setfiletype vim
    autocmd FileType *.cob setfiletype cobol
augroup End
